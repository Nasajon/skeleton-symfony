{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "ekreative/health-check-bundle": "^1.8",
        "friendsofsymfony/rest-bundle": "^3.2",
        "jms/serializer": "^3.17",
        "nasajon/login-bundle": "dev-symfony5",
        "nasajon/sdk": "dev-symfony5",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "sensio/framework-extra-bundle": "^6.2",
        "sentry/sentry-symfony": "^4.2",
        "stevenmaguire/oauth2-keycloak": "^2.2",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/framework-bundle": "5.4.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/options-resolver": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/yaml": "5.4.*"
    },
    "require-dev": {
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "nasajon/mda4": "dev-master",
        "nasajon/mdatransformer": "dev-erp4",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.4.27"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Nasajon\\MDABundle\\": "mda/Nasajon/MDABundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:Nasajon/LoginBundle"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/SDK"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/MdaPhpTransformer"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/mda4"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/bancosweb.git"
        }
    ]
}
