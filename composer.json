{
    "name": "nasajon/diretorio",
    "license": "MIT",
    "type": "project",
    "description": "DiretÃ³rio Nasajon Sistemas",
    "autoload": {
        "psr-4": {
            "Nasajon\\Diretorio\\": "src/Nasajon/Diretorio", 
            "Nasajon\\MDABundle\\": "src/Nasajon/MDABundle"
        },
        "classmap": ["app/AppKernel.php", "app/AppCache.php"]
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "^3.3",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/monolog-bundle": "^3.1.0",
        "sensio/framework-extra-bundle": "^3.0.2",      
        "jms/serializer-bundle": "^2.3",
        "friendsofsymfony/rest-bundle": "^2.3",  
        "friendsofsymfony/jsrouting-bundle": "^1.5",
        "firebase/php-jwt": "2.*",
        "exercise/htmlpurifier-bundle": "^0.2.2",
        "nasajon/SDK": "dev-master",
        "nasajon/login-bundle": "dev-master",
        "nasajon/diretorio-model-bundle": "dev-master",
        "sentry/sentry-symfony": "^2.0",
        "nasajon/componentes-interface-bundle": "dev-master",
        "lightsaml/sp-bundle": "^1.1",
        "doctrine/doctrine-migrations-bundle": "^1.3"
    },
    "require-dev": {
        "sensio/distribution-bundle": "^5.0.19",
        "nasajon/mdatransformer": "dev-master",
        "codeception/codeception" : "^2.3.7"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.1"
        },
        "process-timeout": 3600
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",        
        "symfony-bin-dir": "bin",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:Nasajon/LoginBundle"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/SDK"
        },
        {
            "type": "git",
            "url": "git@github.com:Nasajon/DiretorioModelBundle"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/ComponentesInterfaceBundle"
        },
        {
            "type": "git",
            "url": "https://github.com/Nasajon/MdaPhpTransformer"
        }
    ]
}
